generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Pals {
  idPals       Int @id @default(autoincrement())
  Number String
  Name         String
  Url String
  elements Element[]
  PartnerSkills PartnerSkill[]
  WorksFor WorksFor []
  Drops Drop[]
}

model Element {
  idElement Int @id @default(autoincrement())
  Name String
  pals Pals[]
}

model PartnerSkill {
  idPartnerSkill Int @id @default(autoincrement())
  libelle String
  pals Pals[]
}

model WorksFor {
  idWorksFor Int @id @default(autoincrement())
  libelle String
  pals Pals[]
}

model Drop {
  idDrop Int @id @default(autoincrement())
  Name String
  pals Pals[]
}