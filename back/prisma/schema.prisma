generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Pals {
  ID           Int    @id @default(autoincrement())
  Name         String
  Number       Int
  Element      String
  PartnerSkill String
  WorksFor     String
  Drops        String
}

model User {
  ID Int @id @default(autoincrement())
  Name String 
  Email String 
  Password String 
}
